{
    "buildCommand": "yarn install && ./node_modules/vite/bin/vite.js build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
      {
        "source": "/backend/(.*)",
        "destination": "/backend/server.js"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  